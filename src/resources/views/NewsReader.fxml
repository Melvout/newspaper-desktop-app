<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="articleTitle" alignment="TOP_LEFT" layoutX="304.0" layoutY="30.0" lineSpacing="1.1" prefHeight="139.0" prefWidth="221.0" text="Title" textAlignment="CENTER">
      <font>
         <Font name="System Bold" size="22.0" />
      </font>
   </Label>
   <ListView fx:id="articlesList" layoutX="25.0" layoutY="30.0" onMouseClicked="#articleSelected" prefHeight="354.0" prefWidth="256.0" />
   <ImageView fx:id="articleImage" fitHeight="169.0" fitWidth="239.0" layoutX="537.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true" />
   <WebView fx:id="articleBody" layoutX="307.0" layoutY="220.0" prefHeight="348.0" prefWidth="467.0" />
   <JFXButton id="btn_article_details" fx:id="changeScene" layoutX="133.0" layoutY="402.0" onAction="#openDetailsPage" prefHeight="44.0" prefWidth="148.0" text="Go to article details">
      <font>
         <Font size="14.0" />
      </font>
   </JFXButton>
</AnchorPane>
